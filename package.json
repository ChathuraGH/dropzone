{
  "name": "dropzone",
  "version": "6.0.0-beta.1",
  "description": "Handles drag and drop of files for you.",
  "keywords": [
    "dragndrop",
    "drag and drop",
    "file upload",
    "upload"
  ],
  "homepage": "https://www.dropzone.dev/js",
  "source": "src/dropzone.js",
  "main": "dist/dropzone.js",
  "module": "dist/dropzone.mjs",
  "standalone": "dist/dropzone-min.js",
  "browserslist": "defaults, > 0.25%",
  "targets": {
    "main": {
      "source": "src/dropzone.js"
    },
    "module": {
      "source": "src/dropzone.js"
    },
    "standalone": {
      "source": "tool/dropzone-global.js",
      "outputFormat": "global"
    }
  },
  "maintainers": [
    {
      "name": "Matias Meno",
      "email": "m@tias.me",
      "web": "https://www.yesmeno.com"
    }
  ],
  "contributors": [
    {
      "name": "Matias Meno",
      "email": "m@tias.me",
      "web": "https://www.yesmeno.com"
    }
  ],
  "scripts": {
    "watch": "parcel watch",
    "build": "parcel build && yarn sass src/:dist/ --style compressed",
    "test": "karma start --single-run --browsers ChromeHeadless test/karma.conf.js"
  },
  "bugs": {
    "email": "m@tias.me",
    "url": "https://github.com/dropzone/dropzone/issues"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dropzone/dropzone.git"
  },
  "devDependencies": {
    "@babel/core": "^7.11.0",
    "@babel/preset-env": "^7.11.0",
    "@parcel/transformer-inline-string": "^2.0.0",
    "@parcel/transformer-sass": "^2.0.0",
    "babel-loader": "^8.0.0",
    "chai": "4.3.4",
    "core-js": "^3.8.0",
    "css-loader": "^6.4.0",
    "extract-loader": "^5.1.0",
    "file-loader": "^6.2.0",
    "html-loader": "^2.1.0",
    "karma": "^6.1.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-mocha": "^2.0.1",
    "karma-rollup-preprocessor": "^7.0.5",
    "karma-sinon-chai": "^2.0.2",
    "karma-spec-reporter": "^0.0.32",
    "marked": "^3.0.7",
    "mocha": "^9.1.3",
    "mocha-headless-chrome": "^3.0.0",
    "parcel": "^2.0.0",
    "rollup": "^2.38.4",
    "rollup-plugin-html": "^0.2.1",
    "sass": "^1.33.0",
    "sass-loader": "^12.2.0",
    "sinon": "^11.1.2",
    "sinon-chai": "^3.5.0",
    "webpack": "^5.23.0",
    "webpack-cli": "^4.5.0",
    "webpack-shell-plugin-next": "^2.1.1"
  },
  "dependencies": {
    "@swc/helpers": "^0.2.13"
  }
}
